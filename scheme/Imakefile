#
SHELL=/bin/sh

@SET_MAKE@

top_srcdir=@top_srcdir@
srcdir=@srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bin_dir=@exec_prefix@/bin
scwm_rc=@scwm_rc@
scwm_name=@scwm_name@
scwm_scheme_dir=${prefix}/app/scwm

INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
RM=@RM@

RC=system${scwm_rc}

all: 

depend:

# XXX - this way of doing it sucks. Fix it later when we figure out Automake.
install: 
	${INSTALL_DATA} base.scm ${scwm_dir} 
	${INSTALL_DATA} optargs.scm ${scwm_dir} 
	${INSTALL_DATA} wininfo.scm ${scwm_dir} 
	${INSTALL_DATA} winlist.scm ${scwm_dir} 
	${INSTALL_DATA} winops.scm ${scwm_dir} 

clean:
	${RM} *~ core *.bak

distclean: clean





