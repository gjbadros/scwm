# $Id$

SHELL=/bin/sh

@SET_MAKE@

VPATH=@srcdir@
top_srcdir=@top_srcdir@
srcdir=@srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bin_dir=@exec_prefix@/bin
scwm_rc=@scwm_rc@
scwm_name=@scwm_name@
pixmap_dir=${prefix}/include/X11/pixmaps
mkinstalldirs=$(SHELL) $(top_srcdir)/mkinstalldirs

INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
RM=@RM@

RC=system${scwm_rc}

all: 

depend:

install:
	$(mkinstalldirs) ${pixmap_dir}
	for i in *.xpm; do ${INSTALL_DATA} $$i ${pixmap_dir}; done

mostlyclean:
	${RM} *~ core *.bak

clean: mostlyclean

distclean: clean
