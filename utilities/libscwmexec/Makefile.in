INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
RM=@RM@

LIB=libscwmexec.a

SRCS = libmain.c

OBJS = libmain.o

all: ${LIB}

depend:
	${MAKEDEPEND} ${CFLAGS} ${SRCS}

# don't install the lib for now.
install: all
#	$(mkinstalldirs) ${bin_dir}
#	${INSTALL_PROGRAM} ${PROG} ${bin_dir}

mostlyclean:
	${RM} ${OBJS} *~ core *.bak

clean: mostlyclean
	${RM} ${PROG}

distclean: clean


${LIB}: ${OBJS}
	${AR} ${LIB} ${OBJS}
	${RANLIB} ${LIB}


