XEXT_LIBRARY=-lXext
XPM_LIBRARY=-lXpm
LIBS=-L../libscwmexec ${x_libraries} ${XPM_LIBRARY} ${XEXT_LIBRARY} ${X_LIBRARY} ${LDFLAGS} -lscwmexec


AR=@AR@
