#
SHELL=/bin/sh

@SET_MAKE@

top_srcdir=@top_srcdir@
srcdir=@srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bin_dir=@exec_prefix@/bin
scwm_rc=@scwm_rc@
scwm_name=@scwm_name@
scwm_dir=@scwm_dir@

INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
RM=@RM@

RC=system${scwm_rc}

all: 

depend:

install: 
	if [ ! -e ${scwm_dir} ]; then
	mkdir -p ${scwm_dir}
	fi
	${INSTALL_DATA} ${RC} ${scwm_dir} 

clean:
	${RM} *~ core *.bak

distclean: clean


