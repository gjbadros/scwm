#
SHELL=/bin/sh

@SET_MAKE@

VPATH=@srcdir@
top_srcdir=@top_srcdir@
srcdir=@srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bin_dir=@exec_prefix@/bin
scwm_rc=@scwm_rc@
scwm_name=@scwm_name@
scwm_dir=@scwm_dir@
mkinstalldirs=$(SHELL) $(top_srcdir)/mkinstalldirs

INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
RM=@RM@

RC=system${scwm_rc}

all: 

depend:

install: 
	$(mkinstalldirs) ${scwm_dir}
	${INSTALL_DATA} ${RC} ${scwm_dir} 

mostlyclean:
	${RM} *~ core *.bak

clean: mostlyclean

distclean: clean


